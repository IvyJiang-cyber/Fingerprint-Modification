{
    "manifest_version": 2,
    "name": "Defense", 
    "version": "1.0",
    "description": "Prevent Web Tracking",
    "browser_action": {
        "default_title": "Fingerprint",
        "default_popup": "popup.html"
    },
   "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["js/CanvasModification.js"],
            "run_at": "document_start"
        }
    ],
    "permissions" : [ "tabs", "activeTab"]
}

